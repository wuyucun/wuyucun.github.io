  "files": [
    "lib"
  ],
  "peerDependencies": {
    "vuepress": "2.0.0-rc.26"
  },
  "dependencies": {
    "@vuepress/helper": "2.0.0-rc.121",
    "@vueuse/core": "^14.1.0",
    "@vueuse/integrations": "^14.1.0",
    "chokidar": "5.0.0",
    "focus-trap": "^7.8.0",
    "mark.js": "^8.11.1",
    "minisearch": "^7.2.0",
    "p-map": "^7.0.4",
    "vue": "^3.5.27"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keyword": [
    "VuePress",
    "vuepress plugin",
    "mini search",
    "vuepress-plugin-search"
  ],
  "scripts": {
    "build": "pnpm run tsdown && pnpm run copy",
    "clean": "rimraf --glob ./lib",
    "copy": "cpx \"src/**/*.{d.ts,vue,css,scss,jpg,png}\" lib",
    "tsdown": "tsdown"
  }
