#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vue/yunriver/node_modules/.pnpm/millify@6.1.0/node_modules/millify/bin/node_modules:/mnt/d/vue/yunriver/node_modules/.pnpm/millify@6.1.0/node_modules/millify/node_modules:/mnt/d/vue/yunriver/node_modules/.pnpm/millify@6.1.0/node_modules:/mnt/d/vue/yunriver/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/vue/yunriver/node_modules/.pnpm/millify@6.1.0/node_modules/millify/bin/node_modules:/mnt/d/vue/yunriver/node_modules/.pnpm/millify@6.1.0/node_modules/millify/node_modules:/mnt/d/vue/yunriver/node_modules/.pnpm/millify@6.1.0/node_modules:/mnt/d/vue/yunriver/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../millify@6.1.0/node_modules/millify/bin/millify" "$@"
else
  exec node  "$basedir/../../../../../../millify@6.1.0/node_modules/millify/bin/millify" "$@"
fi
