      "optional": true
    },
    "ogl": {
      "optional": true
    },
    "pinyin-pro": {
      "optional": true
    },
    "postprocessing": {
      "optional": true
    },
    "swiper": {
      "optional": true
    },
    "three": {
      "optional": true
    }
  },
