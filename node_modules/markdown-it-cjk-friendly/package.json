{
  "name": "markdown-it-cjk-friendly",
  "version": "2.0.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "url": "https://github.com/tats-u/markdown-cjk-friendly"
  },
  "license": "MIT",
  "author": "Tatsunori Uchino <tats.u@live.jp> (https://github.com/tats-u)",
  "bugs": "https://github.com/tats-u/markdown-cjk-friendly/issues",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "japanese",
    "chinese",
    "korean",
    "cjk"
  ],
  "description": "markdown-it plugin to make Markdown emphasis (`**`) in CommonMark more friendly with Chinese, Japanese, and Korean (CJK)",
  "sideEffects": false,
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "markdown-it": "^14.1.0"
  },
  "peerDependencies": {
    "@types/markdown-it": "*",
    "markdown-it": "*"
  },
  "peerDependenciesMeta": {
    "@types/markdown-it": {
      "optional": true
    }
  },
  "dependencies": {
    "get-east-asian-width": "^1.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "build:lib": "tsdown",
    "dev": "tsdown --watch",
    "dev:lib": "tsdown --watch",
    "test": "vitest run",
    "test:lib": "vitest run",
    "test:up": "vitest -u",
    "test:watch": "vitest watch",
    "test:lib:watch": "vitest watch",
    "bench": "vitest bench --run",
    "bench:watch": "vitest bench",
    "lint:type": "tsc --noEmit"
  }
}